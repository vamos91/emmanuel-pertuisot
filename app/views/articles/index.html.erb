    <div class="row well">
      <% @articles.each do |article| %>
      <div class="col-xs-12 col-sm-6 col-md-8">
        <div class="card">
        <%= cl_image_tag article.photo, width: "100%" %>
        <%= article.title %>
        <%= article.description %>
        <%= article.created_at %>
        </div>
      </div>
      <% end %>
      <div class="col-xs-12 col-sm-4 col-md-4">

      </div>
    </div>


